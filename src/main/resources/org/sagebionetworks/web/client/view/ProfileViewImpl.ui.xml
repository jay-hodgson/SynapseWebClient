<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view">
	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
	
	<g:HTMLPanel>
		<div class="color-line"></div>
		<g:SimplePanel ui:field="header" />
		<div class="separator margin-top-10-imp"></div>
		<div class="margin-10">
			<div class="row margin-top-5">
				<div class="col-md-3" style="white-space: normal;">
					<g:SimplePanel ui:field="breadcrumbsPanel" />
				</div>
				<div class="col-md-9">
					<g:FlowPanel ui:field="editProfileButtonPanel" />
				</div>
			</div>

			<div class="row">
				<div class="col-md-3 col-sm-4 col-xs-12">
						<g:SimplePanel ui:field="picturePanel"/>
				</div>
				<div class="col-md-9 col-sm-8 col-xs-12">
					<g:SimplePanel ui:field="viewProfilePanel" />
				</div>
			</div>
			<div class="row margin-top-5">
				<div class="col-xs-12">
					<g:Anchor ui:field="showProfileLink" text="Show my full profile" />
				</div>
			</div>
			<div class="row margin-top-5">
			    <div class="col-xs-12">
				    <div class="row" ui:field="navtabContainer">
					    <ul class="nav nav-tabs entity-tabs margin-top-15">				        
						    <li class="entity-tab active margin-left-15" ui:field="projectsListItem"><g:Anchor styleName="text-align-center no-background" ui:field="projectsLink"/></li>
						    <li class="entity-tab" ui:field="challengesListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="challengesLink"/></li>
						    <li class="entity-tab" ui:field="teamsListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="teamsLink"/></li>
						    <!--  <li class="entity-tab" ui:field="messagesListItem" style="width: 157px"><g:Anchor styleName="text-align-center link no-background" ui:field="messagesLink"/></li> -->
						    <li class="entity-tab" ui:field="settingsListItem" style="width: 157px"><g:Anchor styleName="text-align-center link no-background" ui:field="settingsLink"/></li>
					    </ul>
					</div>
					<div class="row">
						<div class="tab-background padding-top-bottom-15-left-right-25" ui:field="projectsTabContainer">
							<div ui:field="createProjectUI" class="margin-bottom-10 row">
							    <div class="col-xs-12 col-sm-6">
									<div class="input-group">
										<g:TextBox ui:field="createProjectTextBox" styleName="form-control" />
										<span class="input-group-btn">
											<g:Button ui:field="createProjectButton" styleName="btn btn-default">Create Project</g:Button>
										</span>
									</div>
								</div>
								<div class="col-xs-12 col-sm-6" />
								<div class="col-xs-12 left">
								    <a href="#!ProjectsHome:0">What's a Project?</a>
								</div>
							</div>
							<div class="highlight-box" highlight-box-title="Projects">
							    <div class="row">
								    <div ui:field="projectTypesUI" class="col-xs-12 col-md-3">
								        <ul class="well nav margin-top-0 bs-sidenav">
								            <li ui:field="projectTypeAllListItem"><g:Anchor ui:field="projectTypeAll" text="All"/></li>
								            <li ui:field="projectTypeAdministratorListItem"><g:Anchor ui:field="projectTypeAdministrator" text="My Projects" /></li>
								            <!-- <li ui:field="projectTypeSharedListItem"><g:Anchor ui:field="projectTypeShared" text="Shared with me" /></li> -->
								            <li ui:field="projectTypeFavoritesListItem"><g:Anchor ui:field="projectTypeFavorites" text="Favorites"/></li>
								        </ul>
								    </div>
									<g:FlowPanel ui:field="projectsTabContent" />
								</div>
							</div>
							
						</div>
					    <div class="tab-background padding-15" ui:field="challengesTabContainer">
					    	<div ui:field="challengesHighlightBox" class="highlight-box" highlight-box-title="Challenges">
					    		<g:FlowPanel ui:field="challengesTabContent" />
					    	</div>
					    <!-- Carousel
						    ================================================== -->
						    <div id="myCarousel" class="carousel slide margin-top-10">
						      <!-- Indicators -->
						      <ol class="carousel-indicators">
						        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
						        <li data-target="#myCarousel" data-slide-to="1"></li>
						        <li data-target="#myCarousel" data-slide-to="2"></li>
						        <li data-target="#myCarousel" data-slide-to="3"></li>
							  </ol>
						      <div class="carousel-inner">
						        <div class="item active">
						          <a href="#!Challenges:DREAM"><g:Image resource='{sageImageBundle.dream85Banner}' /></a>
						          <div class="container">
						            <div class="carousel-caption">
						              <p><a class="btn btn-large btn-primary" href="#!Challenges:DREAM">Learn More</a></p>
						            </div>
						          </div>
						        </div>
						        <div class="item">
						          <a href="#!Synapse:syn312572"><g:Image resource='{sageImageBundle.dream85SM}' /></a>
						          <div class="container">
						            <div class="carousel-caption">			              
						              <p><a class="btn btn-large btn-primary" href="#!Synapse:syn312572">Challenge Details &amp; Registration</a></p>
						            </div>
						          </div>
						        </div>
						        <div class="item">
						          <a href="#!Synapse:syn2455683"><g:Image resource='{sageImageBundle.dream9AML}' /></a>
						          <div class="container">
						            <div class="carousel-caption">
						              <p><a class="btn btn-large btn-primary" style="position: relative; top: -45px; left: 200px;" href="#!Synapse:syn2455683">Challenge Details &amp; Registration</a></p>
						            </div>
						          </div>
						        </div>
						        <div class="item">
						          <a href="#!Synapse:syn2290704"><g:Image resource='{sageImageBundle.dream9AD}' /></a>
						          <div class="container">
						            <div class="carousel-caption">
						              <p><a class="btn btn-large btn-primary" style="position: relative; top: -60px;" href="#!Synapse:syn2290704">Challenge Details &amp; Registration</a></p>
						            </div>
						          </div>
						        </div>
						        <div class="item">
						          <a href="#!Synapse:syn2384331"><g:Image resource='{sageImageBundle.dream9Broad}' /></a>
						          <div class="container">
						            <div class="carousel-caption">
						                <p><a class="btn btn-large btn-primary" style="position: relative; top: -64px;" href="#!Synapse:syn2384331">Challenge Details &amp; Registration</a></p>
						            </div>
						          </div>
						        </div>
						      </div>
						      <a class="left carousel-control-clean" href="#myCarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
						      <a class="right carousel-control-clean" href="#myCarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
						    </div><!-- /.carousel -->
					    </div>
					    <div class="tab-background padding-15" ui:field="teamsTabContainer">
							<div ui:field="createTeamUI" class="margin-bottom-10 row">
							    <div class="col-xs-12 col-sm-6">
									<div class="input-group">
										<g:TextBox ui:field="createTeamTextBox" styleName="form-control" />
										<span class="input-group-btn">
											<g:Button ui:field="createTeamButton" styleName="btn btn-default">Create Team</g:Button>
										</span>
									</div>
								</div>
								<div class="col-xs-12 col-sm-6" />
							</div>
							<g:FlowPanel ui:field="teamsTabContent" />
						</div>
					    <g:FlowPanel styleName="tab-background padding-top-bottom-15-left-right-25" ui:field="messagesTabContainer" />
					    <g:FlowPanel styleName="tab-background padding-top-bottom-15-left-right-25" ui:field="settingsTabContainer" />
						
					</div>
				</div>
			</div>
			<div class="row margin-top-5">
			    <div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 margin-bottom-20">
				<g:SimplePanel ui:field="updateWithLinkedInPanel" />
			</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 margin-bottom-10">
					<g:SimplePanel ui:field="editPicturePanel"/>
				</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 margin-bottom-20">
					<g:SimplePanel ui:field="editPictureButtonPanel"/>
				</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10">
					<g:SimplePanel ui:field="updateUserInfoPanel" />
				</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10">
					<g:SimplePanel ui:field="certificatePanel" />
				</div>
			</div>
			<div class="row margin-top-5">				
				
			</div>
			<p>&nbsp;</p>
		</div>

		<!-- start footer -->
		<div class="clear"></div>
		<g:SimplePanel ui:field="footer" />
		<!-- end footer -->

	</g:HTMLPanel>
</ui:UiBinder> 