<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<bh:Div>
		<b:Alert ui:field="alert" type="DANGER" addStyleNames="margin-bottom-10 margin-top-10">
			<b:Heading size="H4" text="This content is generated by the user and may be unsafe"/>
			<bh:Paragraph text="Sage Bionetworks has no control over this content, and showing it may harm your computer." />
			<b:Button ui:field="showContentButton" text="Show content (not recommended)" />
		</b:Alert>
		<bh:Div ui:field="container" />
		<bh:Div ui:field="synAlertContainer" />
	</bh:Div>
	
</ui:UiBinder>