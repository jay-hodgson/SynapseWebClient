<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b3="urn:import:org.gwtbootstrap3.extras.slider.client.ui">
	<bh:Div>
		<bh:Div addStyleNames="margin-top-15 margin-bottom-5">
			<bh:Strong ui:field="columnName"/>
		</bh:Div>
		<bh:Div ui:field="radioButtonContainer" />
		<g:HorizontalPanel ui:field="rangeUI" verticalAlignment="ALIGN_MIDDLE" visible="false">
			<b:TextBox ui:field="minField" width="105px"/>
			<bh:Span text="to" addStyleNames="margin-left-5 margin-right-5"/>
			<b:TextBox ui:field="maxField" width="105px"/>
			<b:Button ui:field="applyButton" text="Apply" type="LINK" addStyleNames="margin-left-5" />
		</g:HorizontalPanel>
		<bh:Div ui:field="synAlertContainer"/>
	</bh:Div>
</ui:UiBinder>